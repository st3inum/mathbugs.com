<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <%
    var title = page.title;

    if (is_archive()){
        title = __('archives');

        if (is_month()){
        title += ': ' + page.year + '/' + page.month;
        } else if (is_year()){
        title += ': ' + page.year;
        }
    } else if (is_category()){
        title = __('category') + ': ' + page.category;
    } else if (is_tag()){
        title = __('tag') + ': ' + page.tag;
    }
    %>
    <title><% if (title){ %><%= title %> | <% } %><%= config.title %></title>
    <% if (config.description){ %>
        <meta name="description" content="<%= config.description %>">
        <meta property="og:description" content="<%= config.description %>">
    <% } %>
    <% if (config.title){ %>
        <meta property="og:site_name" content="<%= config.title %>">
    <% } %>
    <% if (config.author){ %>
        <meta property="article:author" content="<%= config.author %>">
    <% } %>
    <% if (theme.header.favicon){ %>
        <link rel="icon" href="<%- url_for(theme.header.favicon) %>">
    <% } %>
    <%- css('css/minireset.min.css') %>
    <%- css('css/all.min.css') %>
    <%- css('css/csshake.min.css') %>
    <%- css('css/hljs/'+theme.hlcss+'.css') %>
    <%- css('css/jquery.fancybox.min.css') %>
    <%- css('styl/main') %>
    <%- js('js/jquery.min.js') %>
    <%- js('js/highlight.min.js') %>
    <%- js('js/jquery.fancybox.min.js') %>
    <%- js('js/clipboard.min.js') %>
